import java.io.*;
import java.util.*;

class Main {
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		String name = sc.next();
		int cnt = 0;
		
		for(int i=0; i<n; i++){
			String names = sc.next();
			if(names.contains(name)) cnt++;
		}
		sc.close();
		System.out.println(cnt);
	}
}
