#include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int b, i, sum=1;
	cin >> b;
	cout << 1 << " + ";
	for(i=2; i<=(b/2); i++){
		if(b%i == 0){
			cout << i ;
			if(i != (b/2)) cout << " + ";
			sum += i;	
		} 
	}
	cout << " = " << sum;
	return 0 ;
}
